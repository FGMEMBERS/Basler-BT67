<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>blade.ac</path>
 
  <animation>
    <object-name>pale</object-name>   
  </animation>   

  <animation>
    <type>rotate</type>
    <object-name>pale</object-name>
    <condition>
      <not>
        <property>controls/engines/engine[0]/propeller-feather</property>
      </not>
    </condition>
    <property>controls/engines/engine[0]/propeller-pitch</property>
    <interpolation>
<!--
      <entry><ind> 0 </ind><dep> 18 </dep></entry>
      <entry><ind> 1 </ind><dep> 23 </dep></entry>
-->
      <entry><ind> 0 </ind><dep> 20 </dep></entry>
      <entry><ind> 1 </ind><dep> 45 </dep></entry>
    </interpolation>
    <axis>
      <x>  0 </x>
      <y>  0 </y>
      <z> -1 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pale</object-name>
    <condition>
      <property>controls/engines/engine[0]/propeller-feather</property>
    </condition>
    <property>controls/engines/engine[0]/propeller-feather-pos</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 90 </dep></entry>
      <entry><ind> 1 </ind><dep>  0 </dep></entry>
    </interpolation>
    <axis>
      <x>  0 </x>
      <y>  0 </y>
      <z> -1 </z>
    </axis>
  </animation>

</PropertyList>
