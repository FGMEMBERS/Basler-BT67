<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>center_console.ac</path>
  
 
 <animation>
		<object-name>volantTrim</object-name>
		<type>material</type>
		<emission>
			<!--<factor-prop>instrumentation/mfd/emission</factor-prop>-->
			<red>0.5</red>
			<green>0.9</green>
			<blue>0.2</blue>
		</emission>
	</animation>
 

  
<!--Throttle-->
  <animation>
    <type>rotate</type>
    <object-name>Throt_G</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor> 70 </factor>
    <center>
      <x-m> 0.334 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.110 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Throt_D</object-name>
    <property>controls/engines/engine[1]/throttle</property>
    <factor> 70 </factor>
    <center>
      <x-m> 0.334 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.110 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <!--Prop-pitch-->
  <animation>
    <type>rotate</type>
    <object-name>Prop_G</object-name>
    <property>controls/engines/engine[0]/propeller-pitch</property>
    <factor> 80 </factor>
    <center>
      <x-m> 0.334 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.110 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Prop_D</object-name>
    <property>controls/engines/engine[1]/propeller-pitch</property>
    <factor> 80 </factor>
    <center>
      <x-m> 0.334 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.110 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
	
<!--Mixture-->	
  <animation>
    <type>rotate</type>
    <object-name>Mixt_G</object-name>
    <object-name>levierM_G</object-name>
    <object-name>Tube</object-name>
    <property>controls/engines/engine[0]/mixture</property>
    <factor> 80 </factor>
    <center>
      <x-m> 0.334 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.110 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Mixt_D</object-name>
    <object-name>levierM_D</object-name>
    <object-name>Tube.001</object-name>
    <property>controls/engines/engine[1]/mixture</property>
    <factor> 80 </factor>
    <center>
      <x-m> 0.334 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.110 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!--Elevator-trim-->
  <animation>
    <type>rotate</type>    
    <object-name>volantTrim</object-name>    
    <property>controls/flight/elevator-trim</property>
    <factor> 400 </factor>
    <center>
      <x-m> 0.346 </x-m>
      <y-m> -0.116 </y-m>
      <z-m> 0.114 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
 <animation>
    <type>translate</type>    
    <object-name>elev_trim_index</object-name>    
    <property>controls/flight/elevator-trim</property>
    <factor> 0.045 </factor>
    <center>
      <x-m> 0.394 </x-m>
      <y-m> -0.088 </y-m>
      <z-m> 0.106 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> 0 </y>
      <z>  1 </z>
    </axis>
  </animation> 
  
 <!--Aileron-trim-->
  <animation>
    <type>rotate</type>    
    <object-name>Aileron_trim</object-name>    
    <property>controls/flight/aileron-trim</property>
    <factor> 3000 </factor>
    <center>
      <x-m> 0.271 </x-m>
      <y-m> -0.140 </y-m>
      <z-m> -0.040 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
 <animation>
    <type>rotate</type>    
    <object-name>Ail_trim_needle</object-name>    
    <property>controls/flight/aileron-trim</property>
    <factor> 90 </factor>
    <center>
      <x-m> 0.262 </x-m>
      <y-m> -0.095 </y-m>
      <z-m> -0.081 </z-m>
    </center>
    <axis>
      <x>  1 </x>
      <y> 0 </y>
      <z>  0 </z>
    </axis>
  </animation>  
  
  <!--Rudder-trim-->
  <animation>
    <type>rotate</type>    
    <object-name>Rudder_trim</object-name>    
    <property>controls/flight/rudder-trim</property>
    <factor> 3000 </factor>
    <center>
      <x-m> 0.271 </x-m>
      <y-m> 0.142 </y-m>
      <z-m> -0.040 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
 <animation>
    <type>rotate</type>    
    <object-name>Rud_trim_needle</object-name>    
    <property>controls/flight/rudder-trim</property>
    <factor> 90 </factor>
    <center>
      <x-m> 0.262 </x-m>
      <y-m> 0.102 </y-m>
      <z-m> -0.081 </z-m>
    </center>
    <axis>
      <x>  1 </x>
      <y> 0 </y>
      <z>  0 </z>
    </axis>
  </animation> 
  
  
  
  
  
</PropertyList>
