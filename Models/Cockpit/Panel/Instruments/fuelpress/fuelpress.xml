<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>fuelpress.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

    <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.8 </red>
      <green> 0.4 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle1</object-name>
    <property>engines/engine/est-fuelpress</property>
    <interpolation>
      <entry><ind>  0  </ind><dep>   0 </dep></entry>
      <entry><ind>  15 </ind><dep> 177 </dep></entry>
      <entry><ind>  25 </ind><dep> 302 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
    <animation>
    <type>rotate</type>
    <object-name>needle2</object-name>
    <property>engines/engine[1]/est-fuelpress</property>
    <interpolation>
      <entry><ind>  0  </ind><dep>   0 </dep></entry>
      <entry><ind>  15 </ind><dep> 177 </dep></entry>
      <entry><ind>  25 </ind><dep> 302 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
    
</PropertyList>

