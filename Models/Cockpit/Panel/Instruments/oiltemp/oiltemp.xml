<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>oiltemp.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>fond2</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>needleL</object-name>
    <object-name>needleR</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needleL</object-name>
    <object-name>needleR</object-name>
    <object-name>fond2</object-name>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.8 </red>
      <green> 0.4 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>needleL</object-name>
    <property>/engines/engine/oil-temperature-degf</property>
    <interpolation>
      <entry><ind> -58 </ind><dep>  -10 </dep></entry>
      <entry><ind>  32 </ind><dep>  -45 </dep></entry>
      <entry><ind> 122 </ind><dep>  -90 </dep></entry>
	    <entry><ind> 302 </ind><dep> -154 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.023 </y-m>
      <z-m>  0.000</z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>needleR</object-name>
    <property>/engines/engine[0]/oil-temperature-degf</property>
    <interpolation>
      <entry><ind> -58 </ind><dep>  -10 </dep></entry>
      <entry><ind>  32 </ind><dep>  -45 </dep></entry>
      <entry><ind> 122 </ind><dep>  -90 </dep></entry>
	    <entry><ind> 302 </ind><dep> -154 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.023 </y-m>
      <z-m> 0.000 </z-m>  
    </center>
    <axis> 
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>

