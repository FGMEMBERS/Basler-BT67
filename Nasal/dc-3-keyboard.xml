<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <key n="100">
    <name>d</name>
    <desc>Crew door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.doorsystem.crewexport();</script>
    </binding>
  </key>

  <key n="68">
    <name>D</name>
    <desc>Passanger door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.doorsystem.passengerexport();</script>
    </binding>
  </key>

  <key n="69">
    <name>E</name>
    <desc>Cargo door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.doorsystem.cargoexport();</script>
    </binding>
  </key>

  <key n="10">
    <name>Ctrl-j</name>
    <desc>Jump signal</desc>
    <binding>
      <command>property-assign</command>
      <property>controls/jump-signal</property>
      <value>true</value>
    </binding>
  </key>

</PropertyList>

